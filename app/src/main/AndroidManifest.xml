<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="widget.kaedea.com.swipeloadingview">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".demo.SimpleSwipeActivity" android:label="Swipe Loading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".demo.EndlessSwipeActivity" android:label="Endless Swipe">
        </activity>

        <activity android:name=".demo.VerticalSwipeActivity" android:label="Vertical Swipe">
        </activity>
    </application>

</manifest>
